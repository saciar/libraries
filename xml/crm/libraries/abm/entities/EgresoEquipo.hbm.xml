<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="crm.libraries.abm.entities.EgresoEquipo" table="tx_egreso_equipos">
        <id name="codigo">
            <column name="id_egreso_equipo" />
        </id>
        <property name="codEquipo">
            <column name="codEquipo"/>
        </property>
        <property name="codEgreso">
            <column name="codEgreso"/>
        </property>
    </class>
</hibernate-mapping>
