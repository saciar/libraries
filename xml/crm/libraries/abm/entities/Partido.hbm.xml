<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.Partido" table="MST_PARTIDO">
        <id name="codigo" column="pt_id">
        </id>
        <property name="codigoPartido" column="pt_codpartido" />
        <property name="codigoProvincia" column="pt_codprovincia" />
        <property name="descripcion" column="pt_descrippartido" />
        <property name="activo" column="activo" />
    </class>

</hibernate-mapping>