<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.Ppto_GastoHoteleria" table="TX_GASTOS_HOTELERIA">
        
        <id name="id" column="gh_id">
        	<generator class="increment"/>
        </id>

        <property name="detalle" column="gh_detalle"/>
        <property name="costo" column="gh_costo"/>

    </class>

</hibernate-mapping>