<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.Provincia" table="MST_PROVINCIA">
        <id name="codigo" column="pv_id">
        </id>
        <property name="codigoProvincia" column="pv_codprovincia" />
        <property name="codigoPais" column="pv_codpais" />
        <property name="descripcion" column="pv_descripprovincia" />
        <property name="activo" column="activo" />
    </class>

</hibernate-mapping>