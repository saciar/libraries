<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="crm.libraries.abm.entities.MovimientoEquipo" table="tx_movimiento_equipos">
        <id name="codigo">
            <column name="idtx_movimiento_equipos" />
        </id>
        <property name="codEquipo">
            <column name="codEquipo"/>
        </property>
        <property name="fecha">
            <column name="fecha"/>
        </property>
        <property name="deposito">
            <column name="deposito"/>
        </property>
        <property name="tipo">
            <column name="tipo_movimiento"/>
        </property>
        <property name="operador">
            <column name="operador"/>
        </property>
        <property name="nroOrden">
            <column name="nroOs"/>
        </property>
    </class>
</hibernate-mapping>