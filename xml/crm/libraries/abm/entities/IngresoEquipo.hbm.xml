<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="crm.libraries.abm.entities.IngresoEquipo" table="tx_ingresos_equipos">
        <id name="codigo">
            <column name="id_ingreso" />
        </id>
        <property name="codEquipo">
            <column name="codigoEquipo"/>
        </property>
        <property name="codDeposito">
            <column name="deposito"/>
        </property>
    </class>
</hibernate-mapping>