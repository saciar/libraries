<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="crm.libraries.abm.entities.Egreso" table="tx_egresos">
        <id name="codigo">
            <column name="id_egreso" />
        </id>
        <property name="tipoEgreso">
            <column name="tipo_egreso"/>
        </property>
        <property name="fecha_egreso">
            <column name="fecha_egreso"/>
        </property>
        
        <property name="codUsuario">
            <column name="usuario_resp"/>
        </property>
        <property name="nroppto">
            <column name="nroOS"/>
        </property>
        
        <property name="codChofer">
            <column name="chofer"/>
        </property>
        <property name="codTransporte">
            <column name="transporte"/>
        </property>
    </class>
</hibernate-mapping>