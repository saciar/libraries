<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="crm.libraries.abm.entities.Depositos" table="inv_depositos">
        <id name="codigo" type="string">
            <column name="dep_iddeposito" />
            <generator class="identity" />
        </id>
        <property name="depDescripcion" type="string">
            <column name="dep_descripcion" length="45" not-null="true" />
        </property>
        <property name="depActivo" type="boolean">
            <column name="dep_activo" length="45" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
