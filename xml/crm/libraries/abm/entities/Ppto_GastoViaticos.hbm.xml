<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.Ppto_GastoViaticos" table="TX_GASTOS_VIATICOS">
        
        <id name="id" column="gv_id">
        	<generator class="increment"/>
        </id>

        <property name="detalle" column="gv_detalle"/>
        <property name="costo" column="gv_costo"/>

    </class>

</hibernate-mapping>