<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.MonedaExtranjera" table="MST_MONEDA_EXTRANJERA">
        <id name="codigo" column="me_codigo">
        </id>
        <property name="descripcion" column="me_descripcion" />
        <property name="valor" column="me_valor" />
        <property name="activo" column="me_activo" />
    </class>

</hibernate-mapping>