<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.PrtPptoInstalacion" table="MST_PRT_PPTO_INSTALACION">
        <id name="codigo" column="ppi_id">
        </id>        
        <property name="titulo" column="ppi_desc_abreviada" />
        <property name="descripcion" column="ppi_descripcion" />
        <property name="activo" column="activo" />
    </class>

</hibernate-mapping>