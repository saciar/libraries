<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.Localidad" table="MST_LOCALIDAD">
        <id name="codigo" column="lc_id">
        </id>
        <property name="codigoPartido" column="lc_codpartido" />
        <property name="codigoLocalidad" column="lc_codlocalidad" />
        <property name="descripcion" column="lc_descriplocalidad" />
        <property name="activo" column="activo" />
    </class>

</hibernate-mapping>