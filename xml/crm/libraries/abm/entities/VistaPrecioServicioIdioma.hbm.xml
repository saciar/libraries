<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.VistaPrecioServicioIdioma" table="VW_PRECIOS_SERVICIOS_IDIOMA">
        <id name="codigo" column="ID">
        </id>
        <property name="codigoFamilia" column="CODFAMILIA" />
        <property name="familia" column="FAMILIA" />
        <property name="codigoServicio" column="CODSERVICIO" />
        <property name="servicio" column="SERVICIO" />
        <property name="precioLista" column="PRECIOLISTA" />
        <property name="codigoLugar" column="LUGAR" />
        <property name="variacion" column="VARIACION" />
        <property name="precioLugar" column="PRECIOLUGAR" />
    </class>

</hibernate-mapping>