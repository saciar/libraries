<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="crm.libraries.abm.entities.EstadoEquipos" table="inv_estado_equipos">
        <id name="codigo" column="est_eq_id">
