<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.VistaFamiliaServicioIdioma" table="VW_FAM_SERVICIOS_IDIOMA">
        <id name="codigo" column="ID">
        </id>
        <property name="codigoFamilia" column="CODFAMILIA" />
        <property name="familia" column="DESCFAMILIA" />
        <property name="codigoServicio" column="SERVICIO" />
        <property name="descripcion" column="DESCRIPCION" />
    </class>

</hibernate-mapping>