<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.ListaPrecioCustom" table="MST_LISTAS_CUSTOM">
        <id name="codigo" column="lc_codlista">
        </id>
        <property name="codigoLugar" column="lc_codlugar" />
        <property name="descripcion" column="lc_descripcion" />
        <property name="variacion" column="lc_variacion" />
        <property name="activo" column="activo" />
    </class>

</hibernate-mapping>