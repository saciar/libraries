<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="crm.libraries.abm.entities.Comision" table="MST_COMISIONES">
        <id name="codigo" column="cm_codcomi">
        </id>
        <property name="codigoVendedor" column="cm_codvend" />
        <property name="porcentaje" column="porcentaje" />
        <property name="marcoLiquidacion" column="cm_marco_liquidacion" />
        <property name="activo" column="activo" />
    </class>

</hibernate-mapping>